{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "swcrcPath": "../../packages/swc-config/base.json"
      }
    },
    "tsConfigPath": "./tsconfig.json"
  },
  "entryFile": "main"
}
